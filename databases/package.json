{
  "name": "imageon-databases",
  "version": "1.0.0",
  "description": "DynamoDB connection scripts for ImageonApp",
  "main": "connect-dynamodb.js",
  "scripts": {
    "connect": "node connect-dynamodb.js",
    "connect:ts": "ts-node connect-dynamodb.ts",
    "setup": "aws dynamodb create-table --cli-input-json file://create-imageonapp-table.json --endpoint-url http://localhost:8000",
    "setup:multi": "node setup-multi-tables.js",
    "setup:cloud": "node setup-cloud-tables.js",
    "list-tables": "aws dynamodb list-tables --endpoint-url http://localhost:8000",
    "scan": "aws dynamodb scan --table-name ImageonApp --endpoint-url http://localhost:8000",
    "seed": "aws dynamodb batch-write-item --request-items file://seed-imageonapp.json --endpoint-url http://localhost:8000"
  },
  "dependencies": {
    "aws-sdk": "^2.1691.0",
    "dotenv": "^16.4.5"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  },
  "keywords": [
    "dynamodb",
    "aws",
    "database",
    "local"
  ],
  "author": "Your Name",
  "license": "MIT"
}
